{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "lint": "vue-cli-service lint",
    "dev": "cross-env ENV_NAME=dev webpack-dev-server -c ./build/webpack.config.dev.js",
    "serve": "cross-env ENV_NAME=dev webpack-dev-server -c ./build/webpack.config.dev.js",
    "dev-passenger": "cross-env ENV_NAME=dev-passenger webpack-dev-server -c ./build/webpack.config.dev.js",
    "dev-promotion": "cross-env ENV_NAME=dev-promotion webpack-dev-server -c ./build/webpack.config.dev.js",
    "daily": "cross-env ENV_NAME=daily webpack-dev-server -c ./build/webpack.config.dev.js",
    "daily-passenger": "cross-env ENV_NAME=daily-passenger webpack-dev-server -c ./build/webpack.config.dev.js",
    "daily-promotion": "cross-env ENV_NAME=daily-promotion webpack-dev-server -c ./build/webpack.config.dev.js",
    "build:dev": "cross-env NODE_ENV=production ENV_NAME=dev webpack --config build/webpack.config.prod.js",
    "build:daily": "cross-env NODE_ENV=production ENV_NAME=daily webpack --config build/webpack.config.prod.js",
    "build:daily2": "cross-env NODE_ENV=production ENV_NAME=daily2 webpack --config build/webpack.config.prod.js",
    "build:prepare": "cross-env NODE_ENV=production ENV_NAME=prepare webpack --config build/webpack.config.prod.js",
    "build:online": "cross-env NODE_ENV=production ENV_NAME=online webpack --config build/webpack.config.prod.js",
    "delete-source-map-js": "rm -rf ./dist/js/*.map",
    "deploy:dev": "npm run delete-source-map-js && cross-env ENV_NAME=dev node deploy.js",
    "deploy:daily": "npm run delete-source-map-js && cross-env ENV_NAME=daily node deploy.js",
    "deploy:daily2": "npm run delete-source-map-js && cross-env ENV_NAME=daily2 node deploy.js",
    "deploy:prepare": "npm run delete-source-map-js && cross-env ENV_NAME=prepare node deploy.js",
    "deploy:prepare2": "npm run delete-source-map-js && cross-env ENV_NAME=prepare2 node deploy.js",
    "deploy:online": "npm run delete-source-map-js && cross-env ENV_NAME=online node deploy.js"
  },
